@model BEUProyecto.Persona
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- mobile metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    <!-- site metas -->
    <title>@ViewBag.title</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">
    <!--favicon-->
    <link rel="icon" href="~/images/favicon.ico" type="image/x-icon">

    <link href="~/plugins/simplebar/css/simplebar.css" rel="stylesheet" />
    <link href="~/Content/cssComerciante/bootstrap.min.css" rel="stylesheet" />
    <!-- simplebar CSS-->
    <link href="~/Content/cssComerciante/animate.css" rel="stylesheet" type="text/css" />
    <!-- Icons CSS-->
    <link href="~/Content/cssComerciante/icons.css" rel="stylesheet" type="text/css" />
    <!-- Sidebar CSS-->
    <link href="~/Content/cssComerciante/sidebar-menu.css" rel="stylesheet" />
    <!-- Custom Style-->
    <link href="~/Content/cssComerciante/app-style.css" rel="stylesheet" />
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <link rel="stylesheet" href="http://netdna.bootstrapcdn.com/bootstrap/3.0.0-rc2/css/bootstrap-glyphicons.css">)
</head>
<body class="bg-theme bg-theme11">
    <div id="wrapper">
        <div class="clearfix"></div>
        <div class="content-wrapper">
            <div class="container-fluid">
                <div id="sidebar-wrapper" data-simplebar="" data-simplebar-auto-hide="true">
                    <div class="brand-logo">
                        <a href="negocios.html">
                            <img src="~/images/logo.svg" class="logo-icon" alt="logo icon">
                        </a>
                    </div>
                    <ul class="sidebar-menu do-nicescrol">
                        <li class="sidebar-header">MENÚ</li>
                        <li>
                            <a href='@Url.Action("PanelComerciante", "Comerciantes",new { id = ViewBag.id })'>
                                <i class="zmdi zmdi-grid"></i> <span>HOME</span>
                            </a>
                        </li>
                        <li>
                            <a href='@Url.Action("PanelNegocio", "Comerciantes",new { id = ViewBag.id })'>
                                <i class="zmdi zmdi-grid"></i> <span>NEGOCIOS</span>
                            </a>
                        </li>
                        <li>
                            <a href='@Url.Action("PanelPedidos", "Comerciantes",new { id = ViewBag.id })'>
                                <i class="zmdi zmdi-view-dashboard"></i> <span>PEDIDOS</span>
                            </a>
                        </li>
                        <li>
                            <a href='@Url.Action("PanelDatos", "Comerciantes",new { id = ViewBag.id })'>
                                <i class="zmdi zmdi-face"></i> <span>DATOS PERSONALES</span>
                            </a>
                        </li>
                    </ul>

                </div>
                <header class="topbar-nav">
                    <nav class="navbar navbar-expand fixed-top">
                        <ul class="navbar-nav mr-auto align-items-center">
                            <li class="nav-item">
                                <a class="nav-link toggle-menu" href="javascript:void();">
                                    <i class="icon-menu menu-icon"></i>
                                </a>
                            </li>
                        </ul>
                        <ul class="navbar-nav align-items-center right-nav-link">
                            <li class="nav-item dropdown-lg">
                                <a class="nav-link dropdown-toggle dropdown-toggle-nocaret waves-effect" href="@Url.Action("PanelPedidos", "Comerciantes",new { id = ViewBag.id })">
                                    <i class="fa fa-bell-o"></i>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link dropdown-toggle dropdown-toggle-nocaret" data-toggle="dropdown">
                                    <span class="user-profile"><img src='@Url.Content("~/images/profile.png")' class="img-circle" alt="user avatar"></span>
                                </a>
                                <ul class="dropdown-menu dropdown-menu-right">
                                    <li class="dropdown-item user-details">
                                        <a href="javaScript:void();">
                                            <div class="media">
                                                <div class="avatar"><img class="align-self-start mr-3" src='@Url.Content("~/images/profile.png")' alt="user avatar"></div>
                                                <div class="media-body">
                                                    <h6 class="mt-2 user-title">@ViewBag.nombres</h6>
                                                    <p class="user-subtitle">@ViewBag.correo</p>
                                                </div>
                                            </div>
                                        </a>
                                    </li>
                                    <li class="dropdown-divider"></li>
                                    <li class="dropdown-item"><i class="icon-settings mr-2"></i> <a href='@Url.Action("PanelDatos", "Comerciantes",new { id = ViewBag.id })'>Información</a>
                                    <li class="dropdown-divider"></li>
                                    <li class="dropdown-item"><i class="icon-power mr-2"></i><a href='@Url.Action("Login", "Home")'>Cerrar Sesión</a>
                                </ul>
                            </li>
                        </ul>
                    </nav>
                </header>
                @RenderBody()
                <div class="overlay toggle-menu"></div>
            </div>
            <a href="javaScript:void();" class="back-to-top"><i class="fa fa-angle-double-up"></i> </a>
        </div>
    </div>
    <script src="~/Scripts/jsComerciante/jquery.min.js"></script>
    <script src="~/Scripts/jsComerciante/popper.min.js"></script>
    <script src="~/Scripts/jsComerciante/bootstrap.min.js"></script>


    <!-- simplebar js -->
    <script src="~/plugins/simplebar/js/simplebar.js"></script>
    <!-- sidebar-menu js -->
    <script src="~/Scripts/jsComerciante/sidebar-menu.js"></script>

    <!-- Custom scripts -->
    <script src="~/Scripts/jsComerciante/app-script.js"></script>

</body>
</html>
