@model IEnumerable<BEUProyecto.Lista>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.totalPagar)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.totalPagar)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idLista }) |
            @Html.ActionLink("Details", "Details", new { id=item.idLista }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idLista })
        </td>
    </tr>
}

</table>
